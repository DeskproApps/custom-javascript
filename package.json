{
  "name": "@deskproapps/custom-javascript",
  "version": "0.0.3",
  "description": "Allows admins to use HTML, JavaScript, and CSS to create custom apps.",
  "main": "lib/main/javascript/index.js",
  "scripts": {
    "dev": "dpat server .",
    "package": "dpat clean . && dpat compile . && dpat verify ./dist && dpat package .",
    "test": "dpat test ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskproapps/custom-javascript.git"
  },
  "keywords": [
    "deskpro",
    "html",
    "javascript"
  ],
  "author": {
    "name": "DeskPRO Apps",
    "email": "apps@deskpro.com",
    "url": "https://www.deskpro.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/deskproapps/custom-javascript/issues"
  },
  "homepage": "https://github.com/deskproapps/custom-javascript",
  "deskpro": {
    "version": "2.1.0",
    "title": "Custom JavaScript",
    "isSingle": true,
    "scope": "agent",
    "storage": [
      {
        "name": "settings",
        "isBackendOnly": false,
        "permRead": "EVERYBODY",
        "permWrite": "OWNER"
      }
    ],
    "targets": [
      {
        "target": "ticket-sidebar",
        "url": "html/index.html"
      }
    ],
    "settings": [],
    "deskproApiTags": [],
    "externalApis": []
  },
  "dependencies": {
    "@deskpro/apps-sdk-react": "^0.1.6",
    "@deskpro/react-components": "^1.1.2",
    "handlebars": "^4.0.11",
    "taboverride": "^4.0.3"
  },
  "devDependencies": {
    "@deskpro/apps-dpat": "^0.9.1",
    "@deskpro/apps-sdk-core": "^1.0.0-beta.22",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.11.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "url-loader": "^0.5.9"
  }
